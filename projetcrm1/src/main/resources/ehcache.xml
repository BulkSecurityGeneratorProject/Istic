<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.projet.crm.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.projet.crm.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.projet.crm.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.projet.crm.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.projet.crm.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="com.projet.crm.domain.Stage"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Entreprise"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Entreprise.stages"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Entreprise.contacts"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Contact"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Contact.stages"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Etudiant"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Etudiant.stages"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Enseignant"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.Enseignant.stages"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.PeriodeStage"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="com.projet.crm.domain.PeriodeStage.stages"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
